set cncpcompletion

site '*://mail.google.com/*' {
  unmap j
  unmap k
}

site '*://www.lingq.com/*' {
  unmap l
}

site '*://tinycards.duolingo.com/*' {
  unmap r
}

-> {{
  const extraFilters = {
    '*://www.duolingo.com/*': {
      accept: [
        '._10knO',
      ],
    },
    '*://tinycards.duolingo.com/*': {
      accept: [
        'a',
        '._1vqs9',
        '._3amds'
      ],
    },
    '*://todoist.com/*': {
      accept: [
        '.ist_checkbox',
        '.item_table',
        '.filter',
        '.date'
      ],
    }
  };

  Object.getOwnPropertyNames(extraFilters).forEach(pattern => {
    Hints.siteFilters[pattern] = extraFilters[pattern];
  });
}}

let configpath = 'D:\\Code\\GitHub\\jmbeach\\dot-files\\.chromium-vimrc'
set localconfig
